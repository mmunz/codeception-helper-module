{
    "name": "portrino/codeception-helper-module",
    "description": "Generic helper module for codeception acceptance testing",
    "authors": [
        {
            "name": "Andr√© Wuttig",
            "email": "wuttig@portrino.de"
        }
    ],
    "require": {
        "codeception/codeception": "~2.3 || ^3.1",
        "composer/semver": "^1.4 ",
        "ocramius/package-versions": "~1.1.3",
        "symfony/process": "^5 || ^4 || ^3.3.10 || ~2.7"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "~3.0.2",
        "phpstan/phpstan": "^0.8",
        "phpunit/phpunit": "~6.4"
    },
    "autoload": {
        "psr-4": {
            "Portrino\\Codeception\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Portrino\\Codeception\\Tests\\": "tests"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "vendor-dir": "build/vendor",
        "bin-dir": "build/bin"
    }
}
